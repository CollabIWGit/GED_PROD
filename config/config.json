{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "my-ged-tree-view-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/myGedTreeView/MyGedTreeViewWebPart.js",
          "manifest": "./src/webparts/myGedTreeView/MyGedTreeViewWebPart.manifest.json"
        }
      ]
    },
    "homepage-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/homepage/HomepageWebPart.js",
          "manifest": "./src/webparts/homepage/HomepageWebPart.manifest.json"
        }
      ]
    },
    "doc-details-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/docDetails/DocDetailsWebPart.js",
          "manifest": "./src/webparts/docDetails/DocDetailsWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "MyGedTreeViewWebPartStrings": "lib/webparts/myGedTreeView/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js",
    "HomepageWebPartStrings": "lib/webparts/homepage/loc/{locale}.js",
    "DocDetailsWebPartStrings": "lib/webparts/docDetails/loc/{locale}.js"
  }
}
